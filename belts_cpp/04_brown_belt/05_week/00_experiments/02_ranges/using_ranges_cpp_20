#include <iostream>
#include <ranges>
#include <vector>

using namespace std;

template <rng::InputRange Range>
void Print(const Range& range) {
    for (const auto& x : range) {
        std::cout << x << " ";
    }
    std::cout << std::endl;
}

int main() {
    std::vector v = {4, 1, 7, 2, 3, 8};
    rng::sort(v);
    Print(v);

    return 0;
}